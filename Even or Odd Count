// Even or Odd Count
import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n=sc.nextInt();
        int[] arr=new int[n];
        for(int i=0;i<n;i++){
            arr[i]=sc.nextInt();
        }
        int e=0;
        int o=0;
       for(int i=0;i<n;i++){
           if(arr[i]%2==0){
               e++;
           }else{
               o++;
           }
       }
       System.out.println("Even: "+e+" odd : "+o)
        ;
    }
}
