

import java.util.*;
class factorialzeros{
    public static int factorial(int n){
       int c=0;
        while(n>=5){
            c+=n/5;
            n=n/5;
        }
        return c;
    }
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n=sc.nextInt();
       System.out.println(factorial(n));
       sc.close();
    }
}
